/////////DDL

create table Cart(
    username varchar references "User"(username),
    goods_name varchar references Goods(goods_name),
    PRIMARY KEY (username,goods_name)
);

create table "User"(
    user_id int generated by default as identity primary key,
    username varchar unique,
    email varchar,
    password varchar,
    balance int
);

create table Goods(
    goods_id int generated by default as identity primary key,
    goods_name varchar unique
);

