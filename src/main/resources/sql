create table Cart(
    cart_id int generated by default as identity primary key,
    username varchar,
    goods_name varchar,

);
create table User(
    user_id int generated by default as identity primary key,
    username varchar,
    email varchar,
    password varchar,
    balance int

);
create table Goods(
    goods_id int generated by default as identity primary key,
    goods_name varchar,
    goods_description varchar,
    company varchar,
    goods_price int,
    goods_quantities_in_stock int,
    discount int,
    goods_feedBack varchar,
    goods_keyWord varchar,
    goods_characteristic varchar,
    goods_rate int,


);

create table Company(
    company_id int generated by default as identity primary key,
    name varchar,
    description varchar,
    logo varchar,
    goods varchar
);